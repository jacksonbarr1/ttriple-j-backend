          "type": "string",
          "example": "555-0123"
        },
        "socialLinks": {
          "type": "object",
          "properties": {
            "website": {
              "type": "string",
              "example": "https://example.com"
            },
            "spotify": {
              "type": "string",
              "example": "https://spotify.com/artist/example"
            }
          }
        }
      }
    },
    "Event": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "example": "Summer Music Festival"
        },
        "description": {
          "type": "string",
          "example": "An outdoor music festival"
        },
        "location": {
          "type": "object",
          "properties": {
            "venue": {
              "type": "string",
              "example": "Central Park Amphitheater"
            },
            "address": {
              "type": "string",
              "example": "123 Main St"
            },
            "city": {
              "type": "string",
              "example": "Atlanta"
            },
            "state": {
              "type": "string",
              "example": "GA"
            },
            "country": {
              "type": "string",
              "example": "USA"
            },
            "zipCode": {
              "type": "string",
              "example": "30303"
            },
            "coordinates": {
              "type": "object",
              "properties": {
                "latitude": {
                  "type": "number",
                  "example": 33.749
                },
                "longitude": {
                  "type": "number",
                  "example": -84.388
                }
              }
            }
          }
        },
        "date": {
          "type": "string",
          "example": "2025-07-15T00:00:00.000Z"
        },
        "startTime": {
          "type": "string",
          "example": "18:00"
        },
        "endTime": {
          "type": "string",
          "example": "23:00"
        },
        "images": {
          "type": "array",
          "example": [
            "https://example.com/event1.jpg"
          ],
          "items": {
            "type": "string"
          }
        },
        "tags": {
          "type": "array",
          "example": [
            "festival",
            "outdoor",
            "rock"
          ],
          "items": {
            "type": "string"
          }
        },
        "confirmedBands": {
          "type": "array",
          "example": [],
          "items": {}
        },
        "capacity": {
          "type": "number",
          "example": 500
        },
        "ticketPrice": {
          "type": "number",
          "example": 25
        },
        "organizerName": {
          "type": "string",
          "example": "John Doe"
        },
        "organizerEmail": {
          "type": "string",
          "example": "organizer@example.com"
        },
        "status": {
          "type": "string",
          "example": "upcoming"
        }
      }
