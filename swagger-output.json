  "definitions": {
    "Band": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "example": "The Band Name"
        },
        "description": {
          "type": "string",
          "example": "A description of the band"
        },
        "location": {
          "type": "object",
          "properties": {
            "city": {
              "type": "string",
              "example": "Atlanta"
            },
            "state": {
              "type": "string",
              "example": "GA"
            },
            "country": {
              "type": "string",
              "example": "USA"
            },
            "coordinates": {
              "type": "object",
              "properties": {
                "latitude": {
                  "type": "number",
                  "example": 33.749
                },
                "longitude": {
                  "type": "number",
                  "example": -84.388
                }
              }
            }
          }
        },
        "images": {
          "type": "array",
          "example": [
            "https://example.com/image1.jpg",
            "https://example.com/image2.jpg"
          ],
          "items": {
            "type": "string"
          }
        },
        "tags": {
          "type": "array",
          "example": [
            "rock",
            "indie",
            "alternative"
          ],
          "items": {
            "type": "string"
          }
        },
